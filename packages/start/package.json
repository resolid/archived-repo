    "tsup": "^8.0.2",
    "type-fest": "^4.10.2",
    "vite": "^5.1.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "vite": "^5.1.3"
  },
  "engines": {
    "node": "20.x"
  },
  "repository": {
